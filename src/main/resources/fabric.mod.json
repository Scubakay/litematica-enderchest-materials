{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "An addon for Litematica that adds your Enderchest inventory to the material list.",
  "authors": [
    "Scubakay"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/litematica-enderchest-materials",
    "sources": "https://github.com/Scubakay/litematica-enderchest-materials",
    "issues": "https://github.com/Scubakay/litematica-enderchest-materials/issues"
  },
  "license": "MIT",
  "icon": "assets/litematica-enderchest-materials/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.scubakay.litematicaenderchestmaterials.LitematicaEnderchestMaterials"
    ]
  },
  "mixins": [
    "litematica-enderchest-materials.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15",
    "minecraft": "${mcdep}",
    "litematica": ">=${litematica}",
    "malilib": ">=${malilib}"
  }
}
